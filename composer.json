{
    "name": "novicell/drupal-premium",
    "description": "Novicell Drupal Premium package for Drupal 9 projects",
    "type": "project",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8",
            "exclude": [
                "drupal/migrate_manifest",
                "drupal/promote_disable"
            ]
        },
        {
            "type": "git",
            "url": "https://git.drupalcode.org/issue/migrate_manifest-3288625.git"
        },
        {
            "type": "git",
            "url": "https://git.drupalcode.org/issue/promote_disable-3289171.git"
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.dk/php/"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-pdo": "*",
        "bower-asset/jquery-simple-color": "^1.2",
        "bower-asset/spectrum": "^1.8",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal/address": "^1.10",
        "drupal/admin_toolbar": "^3.3",
        "drupal/adminimal_admin_toolbar": "1.x-dev@dev",
        "drupal/adminimal_theme": "^1.7",
        "drupal/allowed_formats": "^2.0",
        "drupal/autosave_form": "^1.0",
        "drupal/ckeditor": "^1.0",
        "drupal/color": "^1.0",
        "drupal/color_field": "^3.0",
        "drupal/components": "^3.0@beta",
        "drupal/config_ignore": "^2.3",
        "drupal/config_split": "^2.0@RC",
        "drupal/cookiebot": "^1.0.0-alpha8",
        "drupal/core": "^10.0",
        "drupal/core-composer-scaffold": "^10.0",
        "drupal/core-project-message": "^10.0",
        "drupal/core-vendor-hardening": "^9.0",
        "drupal/ctools": "^4.0",
        "drupal/dynamic_entity_reference": "^3.0",
        "drupal/editor_advanced_link": "^2.2",
        "drupal/entity_clone": "^2.0@beta",
        "drupal/entity_embed": "^1.4",
        "drupal/entity_usage": "^2.0-beta3",
        "drupal/field_group": "^3.0",
        "drupal/field_permissions": "^1.2",
        "drupal/file_delete": "^2.0",
        "drupal/focal_point": "^2.0@alpha",
        "drupal/gin": "^3.0@RC",
        "drupal/honeypot": "^2.1",
        "drupal/hreflang": "^1.0",
        "drupal/html5history": "1.x-dev@dev",
        "drupal/imageapi_optimize": "^4.0@beta",
        "drupal/inline_block_title_automatic": "^1.0",
        "drupal/layout_builder_at": "^2.0",
        "drupal/layout_builder_modal": "^1.0",
        "drupal/layout_builder_restrictions": "^2.0",
        "drupal/linkit": "^6.0.0-beta2",
        "drupal/lupus_taxonomy_import": "^1.2",
        "drupal/mailsystem": "^4.3",
        "drupal/media_directories": "^2.1@beta",
        "drupal/media_library_bulk_upload": "^1.0@alpha",
        "drupal/menu_item_extras": "^2.16",
        "drupal/metatag": "^1.0",
        "drupal/migrate_file": "^2.1",
        "drupal/migrate_manifest": "dev-3288625-automated-drupal-10",
        "drupal/migrate_plus": "^6.0",
        "drupal/migrate_tools": "^6.0",
        "drupal/mobile_detect": "^3.0",
        "drupal/noreferrer": "^1.0",
        "drupal/oembed_providers": "^2.1",
        "drupal/paragraphs": "^1.16",
        "drupal/paragraphs_asymmetric_translation_widgets": "^1.2",
        "drupal/pathauto": "^1.11",
        "drupal/profile": "^1.7",
        "drupal/promote_disable": "^2.0",
        "drupal/rdf": "^2.1",
        "drupal/redirect": "^1.0",
        "drupal/redirect_404": "^1.8",
        "drupal/redis": "^1.5",
        "drupal/robotstxt": "^1.4",
        "drupal/role_delegation": "^1.1",
        "drupal/role_theme_switcher": "^1.1",
        "drupal/rules": "^3.0@alpha",
        "drupal/scheduled_transitions": "^2.0",
        "drupal/search_api": "^1.0",
        "drupal/search_api_exclude_entity": "^1.0",
        "drupal/search_api_page": "^1.0",
        "drupal/simple_sitemap": "^4.1",
        "drupal/smtp": "^1.2",
        "drupal/stable": "^2.0",
        "drupal/swiftmailer": "^2.4",
        "drupal/taxonomy_machine_name": "^2.0",
        "drupal/typed_data": "^1.0@beta",
        "drupal/view_unpublished": "^1.1",
        "drupal/webform": "^6.2@RC",
        "drupal/webp": "^1.0@RC",
        "drupal/webp_extras": "^2.0@beta",
        "drupal/workbench_email": "^3.0",
        "drush/drush": "^12",
        "novicell/atoms": "^1.0",
        "novicell/content_hierarchy": "^1.0",
        "novicell/dds_dawa": "^2.0.4",
        "novicell/drupal-premium-theme": "^1.0",
        "novicell/dynamic_key_value": "1.1.9",
        "novicell/entity_overview": "^3.0",
        "novicell/media_icons": "^1.0",
        "novicell/premium_articles": "^3.0",
        "novicell/premium_core": "^1.0",
        "novicell/premium_profile": "^1.0",
        "novicell/premium_tools": "^1.0",
        "novicell/relewise": "^3.0",
        "novicell/status_feed": "^1.0",
        "novicell/styles": "^1.0",
        "oomphinc/composer-installers-extender": "^2.0",
        "vlucas/phpdotenv": "^5.3",
        "wikimedia/composer-merge-plugin": "^2.0"
    },
    "require-dev": {
        "drupal/devel": "^5.1",
        "kint-php/kint": "^5.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "platform": {
            "php": "8.1"
        },
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "drupal/core-vendor-hardening": true,
            "oomphinc/composer-installers-extender": true,
            "wikimedia/composer-merge-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "enable-patching": true,
        "drupal-scaffold": {
            "allowed-packages": [
                "drupal/core"
            ],
            "locations": {
                "web-root": "webroot/"
            },
            "file-mapping": {
                "[web-root]/sites/development.services.yml": {
                    "mode": "replace",
                    "path": "assets/development.services.yml",
                    "overwrite": false
                }
            }
        },
        "installer-paths": {
            "webroot/core": [
                "type:drupal-core"
            ],
            "webroot/libraries/{$name}": [
                "type:drupal-library",
                "type:npm-asset",
                "type:bower-asset"
            ],
            "webroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "webroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "webroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "webroot/drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "webroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "webroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "installer-types": [
            "npm-asset",
            "bower-asset"
        ],
        "merge-plugin": {
            "include": [
                "webroot/modules/contrib/webform/composer.libraries.json"
            ]
        },
        "patches": {
            "drupal/core": {
                "(#2999491) Allow creation of reuseable inline blocks": "https://www.drupal.org/files/issues/2020-05-21/2999491--reusable-title-display--49.patch",
                "(#3080606) Reorder Layout Builder sections": "https://www.drupal.org/files/issues/2020-06-18/3080606-reorder-layout-sections-43.patch",
                "(#2985882) Call to a member function getLabel() after enabling layout_builder": "https://www.drupal.org/files/issues/2020-04-08/2985882-field-85.patch",
                "(#2780839) Optimize migration of specific source IDs for SQL sources": "https://www.drupal.org/files/issues/2022-04-25/2780839-9.4.x-optimize-migrate-source-filter-53.patch",
                "(#3278959) oEmbed resource version as float throws exception": "https://www.drupal.org/files/issues/2022-08-30/media-float-version-3278959-8.patch",
                "Remove csfr requirement from CKEditor5 routes II" : "webroot/sites/skoven-i-skolen.dk/patches/ckeditor5-remove-csfr-requirement-from-routes.patch"
            },
            "drupal/paragraphs": {
                "(#2901390) Integrity constraint violation: 1048 Column 'langcode' cannot be null": "https://www.drupal.org/files/issues/2020-06-25/paragraphs-2901390-51.patch"
            },
            "drupal/cookiebot": {
                "(#3091260) Blockmode 'Auto' will not work in Drupal 8 with core's domready library": "https://www.drupal.org/files/issues/2020-10-08/cookiebot-blocking-auto-mode.patch"
            },
            "drupal/scheduled_transitions": {
                "(#3210877) Get node lock message...": "https://www.drupal.org/files/issues/2021-04-27/scheduled_transitions-3210877-node_lock_message-2.patch"
            },
            "drupal/migrate_manifest": {
                "(#3288625) Automated Drupal 10 compatibility fixes for Migrate Manifest\n": "https://www.drupal.org/files/issues/2023-06-01/migrate_manifest_d10_3288625-6.patch"
            },
            "drupal/promote_disable": {
                "(#3289171) Automated Drupal 10 compatibility fixes": "https://www.drupal.org/files/issues/2022-06-16/promote_disable.2.0.0.rector.patch"
            },
            "drupal/media_directories": {
              "(#3383658) Images in media browser have no hover effect and selected effect in media browser": "https://www.drupal.org/files/issues/2023-08-28/media_directories_ui_browser_css.patch"
            }
        }
    }
}
